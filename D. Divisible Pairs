// https://codeforces.com/contest/1931/problem/D
inline void solve() {
    int n, x, y ; cin >> n >> x >> y ;
    vi a(n);
    for (int i = 0; i < n; ++i) {
        cin >> a[i] ;
    }
    int ans = 0;
    map <pair<int, int>, int> cnt;
    for (int i = 0; i < n; ++i) {
        int r1 = (x - a[i] % x) % x, r2 = (a[i] % y) ;
        ans += cnt[{r1, r2}];
        r1 = a[i] % x;
        r2 = a[i] % y;
        cnt[{r1, r2}] += 1;
    }
    cout << ans << endl;
}








